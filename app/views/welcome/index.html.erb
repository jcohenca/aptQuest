<!-- address search bar -->
<div id="search-form">
  <%= form_tag "/searches/new", method: :get, remote: true do  %>
    <%= label_tag :name, "Enter Address" %>
    <%= text_field_tag :auto, "", id: "autocomplete" %>
    <%= button_tag "Search",  id: "search-button" %>
  <% end %>
</div>



